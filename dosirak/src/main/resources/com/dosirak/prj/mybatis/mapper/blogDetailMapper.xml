  <insert id="insertBlogDetail"
          parameterType="BlogDetailDto">
    INSERT INTO BLOG_DETAIL_T (
        BLOG_LIST_NO
      , BLOG_NO
      , KEYWORD_NO
      , TITLE
      , CONTENTS
      , CREATE_DT
      , MODIFY_DT
    ) VALUES (
        BLOG_LIST_SEQ.NEXTVAL
      , 1
      , 1
      , #{title}
      , #{contents}
      , CURRENT_TIMESTAMP
      , CURRENT_TIMESTAMP
    )
  
  </insert>
  
 
